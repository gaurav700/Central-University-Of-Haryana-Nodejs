<% layout("layouts/boilerplate") -%>

<!-- Welcome Area Start -->
<section class="welcome-area">
    <div class="welcome-slides owl-carousel">
        <!-- Single Slide -->
        <div class="single-welcome-slide bg-img bg-overlay jarallax"
            style="background-image: url(img/bg-img/img19.jpg);">
            <div class="container h-100">
                <div class="row h-100 align-items-center">
                    <!-- Welcome Text -->
                    <div class="col-12">
                        <div class="welcome-text text-right">
                            <h2 data-animation="fadeInUp" data-delay="100ms">Central University <br>Of Haryana</h2>
                            <h6 data-animation="fadeInUp" data-delay="500ms">Jant-Pali , Mahendergarh</h6>
                            <div class="hero-btn-group" data-animation="fadeInUp" data-delay="700ms">
                                <a href="http://cuh.ac.in/" class="btn confer-btn">More Information <i
                                        class="zmdi zmdi-long-arrow-right"></i></a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <!-- Single Slide -->
        <div class="single-welcome-slide bg-img bg-overlay jarallax"
            style="background-image: url(img/bg-img/img7.jpg);">
            <div class="container h-100">
                <div class="row h-100 align-items-center">
                    <!-- Welcome Text -->
                    <div class="col-12">
                        <div class="welcome-text-two text-center">
                            <h5 data-animation="fadeInUp" data-delay="100ms">Central University Of Haryana</h5>
                            <h2 data-animation="fadeInUp" data-delay="100ms">CONFERENCE</h2>

                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!-- Scroll Icon -->
    <div class="icon-scroll" id="scrollDown"></div>
</section>
<!-- Welcome Area End -->

<!-- About Us Start -->
<section class="about-us-countdown-area section-padding-100-0" id="about">
    <div class="container">
        <div class="row align-items-center">
            <!-- About Content -->
            <div class="col-12 col-md-8">
                <div class="about-content-text mb-80">
                    <h6 class="wow fadeInUp" data-wow-delay="100ms">About Conference</h6>
                    <h3 class="wow fadeInUp" data-wow-delay="100ms">Welcome to the Central University Of Haryana
                    </h3>
                    <p class="wow fadeInUp" data-wow-delay="100ms">Lorem ipsum dolor sit amet, consectetur
                        adipiscing elit. Etiam varius mattis massa, ut feugiat erat malesuada eget. Aenean magna
                        urna, bibendum at porta at, luctus vel erat. Orci varius natoque.</p>
                    <a href="#" class="btn confer-btn mt-50 wow fadeInUp" data-wow-delay="100ms">Interested <i
                            class="zmdi zmdi-long-arrow-right"></i></a>
                </div>
            </div>

            <!-- About Thumb -->
            <div class="col-12 col-md-4">
                <div class="about-thumb mb-80 wow fadeInUp" data-wow-delay="100ms">
                    <img src="img/bg-img/img8.png" alt="">
                </div>
            </div>
        </div>
    </div>
</section>
<!-- About Us End -->

<!-- Our Speakings Area Start -->
<section class="our-speaker-area bg-img bg-gradient-overlay section-padding-100-60"
    style="background-image: url(img/bg-img/3.jpg);">
    <div class="container">

        <div class="row">
            <!-- Heading -->
            <div class="col-12">
                <div class="section-heading text-center wow fadeInUp" data-wow-delay="100ms">
                    <p>Our Speakings</p>
                    <h4>Whoâ€™s speaking</h4>
                </div>
            </div>
        </div>

        
            <div class="row">
                <% for(list of speakerList) { %>
                <div class="col-12 col-sm-6 col-lg-4">
                    <div class="single-speaker-area bg-gradient-overlay-2 wow fadeInUp" data-wow-delay="100ms">
                        <!-- Thumb -->
                        <div class="speaker-single-thumb">
                            <img src="<%= list.photo.url %>" alt="<%= list.photo.filename %>">
                        </div>
                        <!-- Social Info -->
                        <div class="social-info">
                            <a href="<%= list.facebook %>"><i class="zmdi zmdi-facebook"></i></a>
                            <a href="<%= list.instagram %>"><i class="zmdi zmdi-instagram"></i></a>
                            <a href="<%= list.twitter %>"><i class="zmdi zmdi-twitter"></i></a>
                            <a href="<%= list.linkedin %>"><i class="zmdi zmdi-linkedin"></i></a>
                        </div>
                        <!-- Info -->
                        <div class="speaker-info">
                            <h5><%= list.name %></h5>
                            <p><%= list.founder %></p>
                        </div>
                    </div>
                </div><%}%>
            </div>
         
        
    </div>
</section>
<!-- Our Speakings Area End -->
