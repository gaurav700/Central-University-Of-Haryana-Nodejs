<% layout("layouts/boilerplateAdmin") -%>

<div class="col-sm-12 mx-3" style="margin-top:120px;">
 <h3 class="text-center">View Inquery Details</h3>
 <table class="table table-bordered">
    <tbody>
     <tr>
      <td>First Name</td>
      <td>
       <%= showContact.fname %>
      </td>
     </tr>
     <tr>
      <td>Last Name</td>
      <td>
          <%= showContact.lname %>
      </td>
     </tr>
     <tr>
      <td>Email</td>
      <td>
          <%= showContact.email %>
      </td>
     </tr>
     <tr>
      <td>Phone No</td>
      <td>
          <%= showContact.phone %> 
      </td>
     </tr>
     <tr>
      <td>Message</td>
      <td>
          <%= showContact.message %>
      </td>
     </tr>
    </tbody>
   </table>
 <div class="text-center">
  <form class='d-print-none d-inline mr-3'><input class='btn btn-success' type='submit' value='Print' onClick='window.print()'></form>
  <form class='d-print-none d-inline' action="/contactadmin"><input class='btn btn-secondary' type='submit' value='Close'></form>
 </div>
</div>


