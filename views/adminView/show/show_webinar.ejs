<% layout("layouts/boilerplateAdmin") -%>

<div class="col-sm-12 mx-3" style="margin-top:120px;">
 <h3 class="text-center">View Webinar Details</h3>

 <table class="table table-bordered">
    <tbody>
     <tr>
      <td>Title</td>
      <td>
       <%= showWebinar.title %>
      </td>
     </tr>
     <tr>
      <td>Organiser</td>
      <td>
          <%= showWebinar.organiser %>
      </td>
     </tr>
     <tr>
      <td>From Date- To Date</td>
      <td>
          <%= formatDate(showWebinar.date) %>
      </td>
     </tr>
     <tr>
      <td>Description</td>
      <td>
          <%= showWebinar.description %>
      </td>
     </tr>
     <tr>
      <td>Link</td>
      <td>
          <%= showWebinar.link %> 
      </td>
     </tr>
     <tr>
      <td>Time</td>
      <td>
          <%= showWebinar.time %>
      </td>
     </tr>
     <tr>
      <td>Venue</td>
      <td>
          <%= showWebinar.venue %>
      </td>
      </tr>
     <tr>
      <td>Photo</td>
     <td>
         <img src="<%= showWebinar.photo.url %>" alt="<%= showWebinar.photo.filename %>" height="100px" width="100px">
     </td>
    </tr>
    </tbody>
   </table>
 </table>
 <div class="text-center">
  <form class='d-print-none d-inline mr-3'><input class='btn btn-success' type='submit' value='Print' onClick='window.print()'></form>
  <form class='d-print-none d-inline' action="/webinarAdmin"><input class='btn btn-secondary' type='submit' value='Close'></form>
 </div>
</div>


