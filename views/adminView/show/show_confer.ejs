<% layout("layouts/boilerplateAdmin") -%>

<div class="col-sm-12 mx-3" style="margin-top:120px;">
 <h3 class="text-center">View Conference Details</h3>
 <table class="table table-bordered">
    <tbody>
     <tr>
      <td>Title</td>
      <td>
       <%= showConfer.title %>
      </td>
     </tr>
     <tr>
      <td>Organiser</td>
      <td>
          <%= showConfer.organiser %>
      </td>
     </tr>
     <tr>
      <td>From Date- To Date</td>
      <td>
          <%= formatDate(showConfer.from_date) %> to
          <%= formatDate(showConfer.to_date) %>
      </td>
     </tr>
     <tr>
      <td>Description</td>
      <td>
          <%= showConfer.description %>
      </td>
     </tr>
     <tr>
      <td>Link</td>
      <td>
          <%= showConfer.link %> 
      </td>
     </tr>
     <tr>
      <td>Time</td>
      <td>
          <%= showConfer.time %>
      </td>
     </tr>
     <tr>
      <td>Venue</td>
      <td>
          <%= showConfer.venue %>
      </td>
      </tr>
      <tr>
       <td>Fees</td>
      <td>
          <%= showConfer.fees %>
      </td>
     </tr>
     <tr>
      <td>Photo</td>
     <td>
         <img src="<%= showConfer.photo.url %>" alt="<%= showConfer.photo.filename %>" height="100px" width="100px">
     </td>
    </tr>
    </tbody>
   </table>
 <div class="text-center">
  <form class='d-print-none d-inline mr-3'><input class='btn btn-success' type='submit' value='Print' onClick='window.print()'></form>
  <form class='d-print-none d-inline' action="/conferAdmin"><input class='btn btn-secondary' type='submit' value='Close'></form>
 </div>
</div>


