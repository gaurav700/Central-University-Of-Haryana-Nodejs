  <link rel="stylesheet" href="/css/default-assets/classy-nav.css">
  <link rel="stylesheet" href="/css/fonts/ElegantIcons.eot">
  <link rel="stylesheet" href="/css/fonts/ElegantIcons.svg">
  <link rel="stylesheet" href="/css/fonts/ElegantIcons.ttf">
